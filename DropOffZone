class DropOffZone {
  constructor(x, y, width, height) {
    var options = {
        'restitution':0,
        'friction':0.3,
        'density':1.0
    }
    this.body = Bodies.rectangle(x, y, width, height, options);
    this.width = width;
    this.height = height;
    
    World.add(world, DropOffZone);
  }
  display();
		var pos =this.DropOffZone.position;
    var angle=this.DropOffZone.angle;
    push();
    translate(330,600)
    rotate(angle);
	}
};
